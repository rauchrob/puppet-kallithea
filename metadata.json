{
  "name": "rauch-kallithea",
  "version": "0.4.0",
  "author": "Robert Rauch (GNS Systems)",
  "summary": "Manage Kallithea, a free source code management system",
  "license": "Apache-2.0",
  "source": "https://github.com/rauchrob/puppet-kallithea",
